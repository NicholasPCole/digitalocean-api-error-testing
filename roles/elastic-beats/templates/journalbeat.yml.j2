cloud.id: {{ elastic_cloud_id }}
cloud.auth: {{ elastic_cloud_auth }}
journalbeat.inputs:
  - paths: []
    seek: cursor
    include_matches:
      - systemd.unit=digitalocean_api_canary.service
      - systemd.unit=digitalocean_api_canary.timer
processors:
  - add_cloud_metadata: ~
  - decode_json_fields:
      fields:
        - message
      target: canary_run
xpack.monitoring.enabled: true
xpack.monitoring.elasticsearch:
